{
  "name": "mafia-game-night",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "npx expo export",
    "build:web": "npx expo export -p web --output-dir web-build && npx copy-files-from-to --config web-build-config.json",
    "predeploy": "node fix-deploy.js",
    "deploy": "npm run build:web && npm run predeploy",
    "deploy:netlify": "npm run build:web && npm run predeploy && netlify deploy --prod"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/slider": "^4.5.5",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.8",
    "expo": "~52.0.35",
    "expo-linear-gradient": "~14.0.2",
    "expo-status-bar": "~2.0.1",
    "firebase": "^11.3.1",
    "react": "18.3.1",
    "react-native": "0.76.7",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "^3.16.7",
    "react-native-safe-area-context": "^4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "^0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "copy-files-from-to": "^3.9.1",
    "gh-pages": "^6.3.0",
    "netlify-cli": "^19.0.3"
  },
  "private": true
}
